    <script>
        $(document).ready(function() {
            $('#table').dataTable({
            });
            $('#accept_btn').click(function(e) {
                $('#accept').modal('show');
            });
            $('#reject_btn').click(function(e) {
                $('#textareaMax').text('');
                $('#reject').modal('show');
            });

            $('#select_all_models').change ( function ( ) {
                if ( $('#select_all_models').prop('checked') ) {
                    $(this).parents('table').find('.models').prop('checked', true);
                } else {
                    $(this).parents('table').find('.models').prop('checked', false);
                }
            });

            $('.dataTables_filter input').attr("placeholder", "Models search...");
        });
    </script>

